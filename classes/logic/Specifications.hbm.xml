<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
		
<hibernate-mapping auto-import="true" default-lazy="false">	
	<class name="logic.Specifications" table="Specifications">
		<id column="car_id" name="carId" type="java.lang.Long">
			<generator class="increment"/>
		</id>
		
		<property column="automatic_transmission" name="autoTransmission" type="java.lang.Boolean"/>
		<property column="engine_power" name="enginePower" type="java.lang.Integer"/>
		<property column="fuel_consumption" name="fuelConsumption" type="java.lang.Double"/>
	</class>
</hibernate-mapping>
